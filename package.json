{
    "name": "boilerplate",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "dev:t": "next dev --turbo",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "format": "prettier --write .",
        "test": "jest --watchAll",
        "test:coverage": "jest test --coverage --watchAll=false  --coverageDirectory=./coverage",
        "test:u": "jest test --watchAll=false -u",
        "type-check": "tsc --pretty --noEmit",
        "prepare": "husky install"
    },
    "dependencies": {
        "@next/font": "^13.0.2",
        "next": "^13.0.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@types/node": "18.11.8",
        "@types/react": "18.0.24",
        "@types/react-dom": "18.0.8",
        "autoprefixer": "^10.4.13",
        "eslint": "8.26.0",
        "eslint-config-next": "^13.0.2",
        "husky": "^8.0.1",
        "jest": "^29.2.2",
        "jest-environment-jsdom": "^29.2.2",
        "postcss": "^8.4.18",
        "postcss-flexbugs-fixes": "^5.0.2",
        "postcss-preset-env": "^7.8.2",
        "prettier": "^2.7.1",
        "tailwindcss": "^3.2.1",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "typescript": "4.8.4"
    }
}
